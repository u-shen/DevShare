<% locals.posts.forEach(post => { %>
 <div class="post-container">
    <div class="post">
      <div class="like">
        <button>❤️</button>
      </div>
      <div class="post-data">
        <div class="title"><%= post.title %></div>
        <div class="url">(<a href="<%= post.url %> "><%= post.url %></a>)</div>
        <div class="comment"><%= post.comment || "10 Comments" %>  </div>
      </div>
    </div>
    <div class="post-meta-data">
        <div class="points">0 points</div>
        <span></span>
        <div class="author">ushen</div>
        <span></span>
        <div class="timestamps"><%= new Date(post.postedAt).toLocaleString()%> </div>
    </div>
 </div>
<% }) %>
